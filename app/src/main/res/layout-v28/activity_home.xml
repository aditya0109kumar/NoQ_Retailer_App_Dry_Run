                    android:text="interested_takeaway" />

                <TextView
                    android:id="@+id/interested_takeaway"
                    android:layout_width="200dp"
                    android:layout_height="30dp"
                    android:text="*******" />


            </LinearLayout>

            <LinearLayout
                android:id="@+id/ll14"
                android:layout_width="match_parent"
