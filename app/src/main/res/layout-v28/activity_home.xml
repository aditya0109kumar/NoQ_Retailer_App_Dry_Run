                    android:text="*******" />


            </LinearLayout>

            <LinearLayout
                android:id="@+id/ll16"
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:layout_below="@+id/ll15"
                tools:ignore="NotSibling">

                <TextView
                    android:layout_width="140dp"
                    android:layout_height="30dp"
                    android:text="Min_Charge" />

                <TextView
                    android:id="@+id/Min_Charge"
                    android:layout_width="200dp"
                    android:layout_height="30dp"
                    android:text="*******" />


            </LinearLayout>

            <LinearLayout
                android:id="@+id/ll17"
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:layout_below="@+id/ll16"
                tools:ignore="NotSibling">

                <TextView
                    android:layout_width="140dp"
                    android:layout_height="30dp"
                    android:text="Max_Charge" />

                <TextView
                    android:id="@+id/Max_Charge"
                    android:layout_width="200dp"
                    android:layout_height="30dp"
                    android:text="*******" />


            </LinearLayout>

            <LinearLayout
                android:id="@+id/ll18"
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:layout_below="@+id/ll17"
                tools:ignore="NotSibling">

                <TextView
                    android:layout_width="140dp"
                    android:layout_height="30dp"
                    android:text="Delivery_Duration" />

                <TextView
                    android:id="@+id/Delivery_Duration"
                    android:layout_width="200dp"
                    android:layout_height="30dp"
                    android:text="*******" />


            </LinearLayout>

            <LinearLayout
                android:id="@+id/ll19"
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:layout_below="@+id/ll18"
                tools:ignore="NotSibling">

                <TextView
                    android:layout_width="140dp"
                    android:layout_height="30dp"
                    android:text="Store_Image" />

                <TextView
                    android:id="@+id/Store_Image"
                    android:layout_width="200dp"
                    android:layout_height="30dp"
                    android:text="*******" />



            </LinearLayout>

        </RelativeLayout>

    </ScrollView>

</RelativeLayout>
